
using System;
namespace Oper
{
    class Program
    {
        static void Main(string[] args)
        {
            int n1;
            Console.Write("Digite o numero ");
            n1 = int.Parse(Console.ReadLine());
            string teste1 = "Par";
            string teste2 = "impar";
       
            ExibirNome(n1 %2 == 0 ? teste1 : teste2);
            Console.ReadKey();
        }
        public static void ExibirNome(string texto)
        {
            Console.WriteLine(texto);
        }

        
    }
}
